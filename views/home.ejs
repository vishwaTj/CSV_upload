<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CSV Upload</title>
</head>
<body>
    <h1>Hello welcome  to CSV Upload</h1>

    <div id="input-form">
     <form class="input-form" action="/" enctype="multipart/form-data" method="post">
        <div>
         <input class="input-element" type="file" name="file" placeholder="upload file">
        </div>
        <div> 
         <input class="input-element" type="submit" value="submit">
        </div> 
     </form>
    </div> 

    <%=message%>

    <div id="Display-data">
        <table class="table">
            <thead class="thead-dark">
              <tr>
                <th scope="col">id</th>
                <th scope="col">FirstName</th>
                <th scope="col">LastName</th>
                <th scope="col">mail</th>
                <th scope="col">Profession</th>
              </tr>
            </thead>
            <tbody>
             <% for(l of list){%>   
              <tr>
                <th scope="row"><%=l.id %></th>
                <td><%= l.firstname %></td>
                <td><%= l.lastname %></td>
                <td><%= l.mail %></td>
                <td><%= l.profession %></td>
              </tr>
              <%}%>
            </tbody>
          </table>
      </div>

<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
